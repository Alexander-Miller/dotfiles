rofi.width:           75
rofi.lines:           10
rofi.columns:         1  
rofi.font:            Fantasque Sans Mono 14
rofi.fg:              #ccb18b
rofi.bg:              #282828
rofi.hlfg:            #000000
rofi.hlbg:            #4c78c9
rofi.bc:              #282828
rofi.bw:              1
rofi.loc:             0
rofi.padding:         20
rofi.yoffset:         0
rofi.xoffset:         0
rofi.fixed-num-lines: false
