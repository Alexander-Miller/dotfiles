URxvt*cursorBlink:     true
URxvt*cursorUnderline: true
URxvt*font:            xft:Fantasque Sans Mono:size=14
URxvt*boldFont:        xft:Fantasque Sans Mono:size=14:bold
URxvt*scrollBar:       false
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher:    firefox
URxvt.matcher.button:  1
URxvt.internalBorder:  16
URxvt.lineSpace:       1
URxvt.letterSpace:     -2

*.foreground: #ccb18b
*.cursorcolor:#ccb18b
*.colorbd:    #ccb18b
*.colorul:    #ccb18b
*.colorit:    #ccb18b
*background:  #2d2d31
*color0:      #1a1a1a
*color1:      #cc6666
*color2:      #66aa66
*color3:      #f0c674
*color4:      #557799
*color5:      #a374a8
*color6:      #339999
*color7:      #a3a3a3
*color8:      #5d5d5d
*color9:      #dd7777
*color10:     #77bb77
*color11:     #f2d785
*color12:     #6688aa
*color13:     #b485b9
*color14:     #2fa7a7
*color15:     #cccccc

rofi.width:           75
rofi.lines:           10
rofi.columns:         1
rofi.font:            Fantasque Sans Mono 14
rofi.fg:              #ccb18b
rofi.bg:              #2d2d31
rofi.hlfg:            #000000
rofi.hlbg:            #557799
rofi.bc:              #2d2d31
rofi.bw:              1
rofi.loc:             0
rofi.padding:         20
rofi.yoffset:         0
rofi.xoffset:         0
rofi.fixed-num-lines: false
rofi.hmode:           false
rofi.terminal:        rxvt-unicode
rofi.ssh-set-title:   true
